| fatorial | NO_ZERO_IN_DATE,NO_ZERO_DATE,NO_ENGINE_SUBSTITUTION | CREATE DEFINER=`root`@`localhost` FUNCTION `fatorial`(N integer) RETURNS int(11)
BEGIN
    DECLARE FATOR, I integer;
    SET FATOR=1;
    SET I=1;
    IF (N<=1) THEN
        RETURN FATOR;
    ELSE
        WHILE(I<=N) DO
            SET FATOR=FATOR*I;
            SET I=I+1;
        END WHILE;
    END IF;
    RETURN FATOR;
END | utf8mb4              | utf8mb4_general_ci   | utf8mb4_general_ci |
